  },
  "homepage": "https://github.com/CoorpAcademy/baucis#readme",
  "peerDependencies": {
    "mongoose": "^6.0.0"
  },
  "devDependencies": {
    "@coorpacademy/eslint-plugin-coorpacademy": "^9.0.1",
    "codecov": "^3.6.1",
    "eslint": "^6.6.0",
    "express": "^4.17.1",
    "lerna": "^6.1.0",
    "mongoose": "^6.8.0",
    "nyc": "^14.1.1"
